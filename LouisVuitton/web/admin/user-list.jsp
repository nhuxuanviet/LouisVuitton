                    title: "Success!",
                    text: "${success}",
                    icon: "success",
                    confirmButtonText: "OK"
                });
            </script>
        </c:if>

        <c:if test="${not empty error}">
            <script>
                Swal.fire({
                    title: "Error!",
                    text: "${error}",
                    icon: "error",
                    confirmButtonText: "OK"
                });
            </script>
        </c:if>

